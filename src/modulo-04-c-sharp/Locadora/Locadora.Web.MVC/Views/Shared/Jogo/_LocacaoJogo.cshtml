@model JogoLocacaoModel

<table class="table table-striped table-bordered table-background">
    <tbody>
        <tr>
            <td width="340px" height="100px" rowspan="4" align="center">
                <img class="table-img" src="@Model.Imagem" alt="@Model.Nome" />
                @Html.Partial("~/Views/Shared/Jogo/_SeloJogo.cshtml", Model.Selo)
            </td>
        </tr>
        <tr>
            <td>@Model.Nome</td>
        </tr>
        <tr>
            <td>@Model.DataEntrega</td>
        </tr>
        <tr>
            <td>@Model.Valor</td>
        </tr>
    </tbody>
</table>

@section Scripts
{
    <script>
        $(function () {
            $('#selo').css('posiion', 'fixed');
        });
    </script>
}
